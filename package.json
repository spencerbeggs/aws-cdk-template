{
	"name": "aws-cdk-template",
	"version": "1.0.0",
	"description": "Just a little boilerplate for setting up Typescript projects the way I like them.",
	"repository": {
		"type": "git",
		"url": "https://github.com/spencerbeggs/aws-cdk-template.git"
	},
	"author": {
		"name": "C. Spencer Beggs",
		"email": "spencer@beg.gs"
	},
	"license": "MIT",
	"private": true,
	"engineStrict": true,
	"engines": {
		"node": "16.14.0"
	},
	"scripts": {
		"bootstrap": "node --no-warnings --experimental-specifier-resolution=node --loader ts-paths-esm-loader ./lib/scripts/bootstrap.ts",
		"engines": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node ./lib/scripts/engines.ts",
		"postinstall": "npm run engines",
		"lint": "eslint . --ext .ts,.mts,.cjs,.js",
		"lint-fix": "eslint . --ext .ts,.mts,.cjs,.js --fix",
		"test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test-ci": "TEST_ENV=ci jest --ci",
		"travis": "jest --coverage --coverageReporters=text-lcov | coveralls"
	},
	"devDependencies": {
		"@octokit/action": "^3.18.0",
		"@octokit/rest": "^18.12.0",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^8.2.0",
		"@types/jest": "^27.4.0",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^17.0.17",
		"@types/semver": "^7.3.9",
		"@types/source-map-support": "^0.5.4",
		"@types/validate-npm-package-name": "^3.0.3",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"constructs": "^10.0.59",
		"coveralls": "^3.1.1",
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"inquirer": "^8.2.0",
		"jest": "^27.5.1",
		"jest-console": "^0.1.0",
		"prettier": "^2.5.1",
		"semver": "^7.3.5",
		"source-map-support": "^0.5.21",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.5.0",
		"ts-paths-esm-loader": "^1.2.2",
		"tsconfig-paths": "^3.12.0",
		"type-fest": "^2.11.2",
		"typescript": "^4.6.0-dev.20220210",
		"typescript-transform-paths": "^3.3.1",
		"validate-npm-package-name": "^3.0.0"
	},
